TWITCH_USERNAME=your_bot_username
TWITCH_OAUTH_TOKEN=oauth:your_oauth_token_here
TWITCH_CHANNELS=channel1,channel2,channel3
LOG_LEVEL=info
DASHBOARD_PORT=3000
RUST_LOG=info
YOUTUBE_API_KEY=your_api_key_here
YOUTUBE_OAUTH_TOKEN=your_oauth_token_here
YOUTUBE_LIVE_CHAT_ID=your_live_chat_id
YOUTUBE_VIDEO_ID=your_video_id

# Adaptive Performance Tuning Configuration
ADAPTIVE_TUNING_ENABLED=true
ADAPTIVE_TUNING_INTERVAL=30
ADAPTIVE_METRICS_RETENTION=24
ADAPTIVE_SAFETY_ENABLED=true
ADAPTIVE_MAX_CHANGES_PER_HOUR=10
ADAPTIVE_ROLLBACK_THRESHOLD=300
ADAPTIVE_LEARNING_MODE=false

# Performance Targets
ADAPTIVE_TARGET_LATENCY=100.0
ADAPTIVE_AGGRESSIVE_LATENCY_THRESHOLD=500.0
ADAPTIVE_TARGET_MEMORY=70.0
ADAPTIVE_CRITICAL_MEMORY=90.0
ADAPTIVE_TARGET_ERROR_RATE=1.0
ADAPTIVE_CRITICAL_ERROR_RATE=5.0

# Dashboard (if you want a separate port for adaptive metrics)
ADAPTIVE_DASHBOARD_PORT=3001

# Safety Settings
ADAPTIVE_CIRCUIT_BREAKER_THRESHOLD=5
ADAPTIVE_CIRCUIT_BREAKER_TIMEOUT=300
ADAPTIVE_SAFETY_SCORE_THRESHOLD=0.3

# Integration Settings
ADAPTIVE_CONFIG_INTEGRATION=true
ADAPTIVE_POOL_INTEGRATION=true
ADAPTIVE_MODERATION_INTEGRATION=true